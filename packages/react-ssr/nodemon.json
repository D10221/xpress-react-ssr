{
    "ignore": [
        "node_modules/**/node_modules",
        "src/components"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --inspect -r ts-node/register"
    },
    "watch": [
        "src/"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts,tsx",
    "delay": 5000
}