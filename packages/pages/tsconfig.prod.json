{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": [
            "dom",
            "esnext"
        ],
        "jsx": "react",
        "sourceMap": false,
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "declaration": false,
        "moduleResolution": "node",
        "baseUrl": "/",
        "preserveSymlinks": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true
    }
}